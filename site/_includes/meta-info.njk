<meta name="viewport" content="width=device-width, initial-scale=1"/>
<meta name="description" content="The CSS implementation of Adobe's Spectrum design system"/>
<meta charset="utf-8"/>
<link rel="icon" type="image/png" href="/favicon.png"/>

<link rel="stylesheet" type="text/css" href="/components/vars/spectrum-global.css"/>
<link rel="stylesheet" type="text/css" href="/components/vars/spectrum-darkest.css"/>
<link rel="stylesheet" type="text/css" href="/components/vars/spectrum-dark.css"/>
<link rel="stylesheet" type="text/css" href="/components/vars/spectrum-light.css"/>
<link rel="stylesheet" type="text/css" href="/components/vars/spectrum-medium.css"/>
<link rel="stylesheet" type="text/css" href="/components/vars/spectrum-large.css"/>
<link rel="stylesheet" type="text/css" href="/components/tokens/index.css"/>

{% for dep in dependencies %}
  <link rel="stylesheet" type="text/css" href='/components/{{ dep }}/index-vars.css' data-dependency={{ dep }} />
{% endfor %}

{% for dep in ['fieldlabel', 'icon', 'link', 'menu', 'picker', 'popover', 'statuslight', 'typography'] %}
  <link rel="stylesheet" type="text/css" href='/components/{{ dep }}/index-vars.css' data-dependency={{ dep }} />
{% endfor %}

<link rel="stylesheet" type="text/css" href="/css/site.css"/>
<link rel="stylesheet" type="text/css" href="/css/docs.css"/>

{# prism css #}
  {# <link rel="stylesheet" type="text/css" href="/css/prism/prism.css" data-prism="data-prism"/> #}
  <link href="https://unpkg.com/prismjs@1.20.0/themes/prism.css" rel="stylesheet">
  {# <link href="https://unpkg.com/prismjs@1.20.0/themes/prism-dark.css" rel="stylesheet"> #}

{% if not process.env.VRT %}
    <link
        rel="stylesheet"
        type="text/css"
        href="//wwwimages2.adobe.com/etc/beagle/public/globalnav/adobe-globalnav/latest/adobe-globalnav.min.css"/>
{% endif %}

<script>{{ component.renderScripts | safe }}</script>
<script src="/js/loadicons/index.js"></script>
<script src="/js/focus-ring-polyfill/index.js"></script>
<script src="/js/lunr/lunr.js"></script>

<script src="/js/typekit.js"></script>
<script src="/js/Search.js"></script>
<script src="/js/site.js"></script>
<script src="/js/docs.js"></script>

{% for dep in dependencies %}
  <script src="/components/{{ dep }}/enhancement.js" data-dependency={{ dep }}></script>
{% endfor %}

<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@mdi/font@6.5.95/css/materialdesignicons.min.css" crossorigin="anonymous" referrerpolicy="no-referrer" />
