{% for dep in [
  'accordion',
  'actionbutton',
  'button',
  'card',
  'clearbutton',
  'divider',
  'fieldlabel',
  'icon',
  'illustratedmessage',
  'link',
  'menu',
  'page',
  'picker',
  'popover',
  'search',
  'sidenav',
  'site',
  'statuslight',
  'table',
  'textfield',
  'tooltip',
  'typography',
  'underlay'
] %}
