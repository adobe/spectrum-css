{
  "name": "@spectrum-css/preview",
  "version": "2.1.3",
  "description": "A Spectrum CSS preview",
  "license": "Apache-2.0",
  "author": "Adobe",
  "homepage": "https://opensource.adobe.com/spectrum-css/preview",
  "main": "main.js",
  "scripts": {
    "prebuild": "run-s build:tokens build:core",
    "build": "build-storybook --config-dir .",
    "build:core": "lerna run --stream --scope \"@spectrum-css/{icon,typography,site}\" build",
    "build:tokens": "lerna run --stream --scope \"@spectrum-css/{vars,expressvars,tokens}\" build",
    "new": "yarn workspace @spectrum-css/generator new story",
    "prestart": "yarn build:core",
    "start": "WATCH_MODE=true start-storybook --config-dir .",
    "test": "chromatic --build-script-name build --junit-report",
    "test:scope": "yarn test --only-story-names"
  },
  "dependencies": {
    "@adobe/spectrum-css-workflow-icons": "^1.2.1",
    "@spectrum-css/expressvars": "^2.0.6",
    "@spectrum-css/icon": "^3.0.35",
    "@spectrum-css/site": "^3.0.9",
    "@spectrum-css/tokens": "^7.7.0",
    "@spectrum-css/vars": "^8.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@etchteam/storybook-addon-status": "^4.2.2",
    "@spectrum-css/component-builder": "^4.0.3",
    "@storybook/addon-a11y": "^6.5.15",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-docs": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addons": "^6.5.15",
    "@storybook/api": "^6.5.15",
    "@storybook/client-api": "^6.5.15",
    "@storybook/components": "^6.5.15",
    "@storybook/core-events": "^6.5.15",
    "@storybook/theming": "^6.5.15",
    "@storybook/web-components": "^6.5.15",
    "@whitespace/storybook-addon-html": "^5.1.1",
    "chromatic": "^6.17.1",
    "lit-html": "^2.6.1",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.36",
    "postcss-class-prefix": "^0.3.0",
    "postcss-loader": "^7.2.0",
    "postcss-prefix-selector": "^1.16.0",
    "postcss-selector-replace": "^1.0.2",
    "postcss-warn-cleaner": "^0.1.9",
    "prettier": "^2.8.1",
    "raw-loader": "^4.0.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "source-map-loader": "^1.0.0",
    "webpack": "^4.46.0"
  }
}
