{
  "name": "@spectrum-css/preview",
  "version": "4.1.14",
  "description": "A Spectrum CSS preview",
  "license": "Apache-2.0",
  "author": "Adobe",
  "homepage": "https://opensource.adobe.com/spectrum-css/preview",
  "main": "main.js",
  "scripts": {
    "build": "yarn storybook",
    "clean": "rimraf storybook-static",
    "new": "yarn workspace @spectrum-css/generator new story",
    "start": "WATCH_MODE=true storybook dev --quiet --config-dir .",
    "storybook": "storybook build --quiet --config-dir .",
    "test": "chromatic --only-changed --build-script-name build --junit-report",
    "test:scope": "yarn test --only-story-names"
  },
  "dependencies": {
    "@adobe/spectrum-css-workflow-icons": "^1.2.1",
    "@spectrum-css/expressvars": "^3.0.9",
    "@spectrum-css/icon": "^4.0.1",
    "@spectrum-css/site": "^4.0.0",
    "@spectrum-css/tokens": "^11.3.3",
    "@spectrum-css/vars": "^9.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@etchteam/storybook-addon-status": "^4.2.4",
    "@spectrum-css/component-builder": "^4.0.14",
    "@storybook/addon-a11y": "^7.3.2",
    "@storybook/addon-actions": "^7.3.2",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-docs": "^7.3.2",
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addon-interactions": "^7.0.20",
    "@storybook/api": "^7.3.2",
    "@storybook/blocks": "^7.0.20",
    "@storybook/client-api": "^7.3.2",
    "@storybook/components": "^7.0.20",
    "@storybook/core-events": "^7.0.20",
    "@storybook/jest": "^0.1.0",
    "@storybook/manager-api": "^7.0.20",
    "@storybook/preview-api": "^7.0.20",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/theming": "^7.0.20",
    "@storybook/web-components-webpack5": "^7.3.2",
    "@whitespace/storybook-addon-html": "^5.1.6",
    "chromatic": "^6.22.0",
    "file-loader": "6.2.0",
    "lit": "^2.8.0",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.36",
    "postcss-class-prefix": "^0.3.0",
    "postcss-loader": "^4.0.0",
    "postcss-prefix-selector": "^1.16.0",
    "postcss-selector-replace": "^1.0.2",
    "postcss-warn-cleaner": "^0.1.9",
    "prettier": "^2.8.8",
    "raw-loader": "^4.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "rimraf": "^5.0.1",
    "source-map-loader": "^1.0.0",
    "storybook": "^7.3.2",
    "storybook-addon-pseudo-states": "^2.1.0",
    "style-loader": "3.3.3",
    "webpack": "^5.83.1"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "inputs": [
          "{workspaceRoot}/assets",
          "{workspaceRoot}/decorators",
          "{workspaceRoot}/*.js",
          "{workspaceRoot}/*.html",
          "{projectRoot}/components/*/dist"
        ],
        "outputs": [
          "{workspaceRoot}/storybook-static"
        ]
      },
      "clean": {
        "inputs": [
          "{workspaceRoot}/storybook-static"
        ]
      }
    },
    "implicitDependencies": [
      "@spectrum-css/*",
      "!@spectrum-css/generator"
    ],
    "includedScripts": [
      "clean",
      "test",
      "test:scope",
      "storybook",
      "build"
    ]
  }
}
