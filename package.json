{
  "name": "spectrum-css-monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "bootstrap": "npx lerna bootstrap --hoist",
    "build": "gulp -LL",
    "dev": "gulp dev",
    "clean": "gulp clean",
    "start": "npm run dev",
    "ci-all": "npm run build",
    "release": "npx lerna publish",
    "version": "gulp version",
    "postinstall": "npm run bootstrap",
    "backstop:approve": "backstop approve --config=backstop_test.js",
    "backstop:test": "backstop test --config=backstop_test.js --env=local",
    "backstop:clean": "rm -rf backstop_data/bitmaps_test backstop_data/html_report",
    "backstop:docker:approve": "backstop approve --docker --config=backstop_test.js --env=local",
    "backstop:docker:test": "backstop test --docker --config=backstop_test.js --env=local",
    "backstop:ci:test": "backstop test --docker --config=backstop_test.js --env=ci",
    "kill-zombies": "pkill -f \"(chrome)?(--headless)\"",
    "prepare": "gulp prepare",
    "postpublish": "npx lerna run publishpages --concurrency=1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/spectrum-css.git"
  },
  "author": "Larry Davis <lazdnet@gmail.com>",
  "contributors": [
    "Garth Braithwaite <garthdb@gmail.com> (https://garthdb.com)",
    "Steve Gill <stevengill97@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/adobe/spectrum-css/issues"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@adobe/focus-ring-polyfill": "^0.1.5",
    "@spectrum-css/spectrum-css-vr-test-asset": "^1.0.0",
    "audit-ci": "^2.0.1",
    "backstopjs": "^4.1.12",
    "gulp": "^4.0.0",
    "gulplog": "^1.0.0",
    "lerna": "^3.16.4",
    "loadicons": "^1.0.0",
    "lunr": "^2.3.6",
    "markdown": "^0.5.0",
    "prismjs": "^1.17.1"
  },
  "optionalDependencies": {
    "@a4u/a4u-collection-essential-ui-icons-release": "^4.1.0",
    "@spectrum/balthazar": "^3.0.1",
    "@spectrum/spectrum-dna": "^5.15.0"
  }
}
