{
	"name": "@spectrum-css/contextualhelp",
	"version": "5.0.1",
	"description": "The Spectrum CSS contextualhelp component",
	"license": "Apache-2.0",
	"author": "Adobe",
	"homepage": "https://opensource.adobe.com/spectrum-css/?path=/docs/components-contextual-help--docs",
	"repository": {
