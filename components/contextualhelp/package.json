		"@spectrum-css/popover": "8.0.1",
		"@spectrum-css/tokens": "14.0.0-next.8"
	},
	"keywords": [
		"design-system",
		"spectrum",
		"spectrum-css",
		"adobe",
		"adobe-spectrum",
		"component",
		"css"
