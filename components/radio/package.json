	"devDependencies": {
		"@spectrum-css/icon": "9.1.0",
		"@spectrum-css/tokens": "14.0.0-next.8"
	},
	"keywords": [
		"design-system",
		"spectrum",
		"spectrum-css",
		"adobe",
		"adobe-spectrum",
		"component",
		"css"
	],
	"publishConfig": {
		"access": "public"
	},
	"spectrum": [
		{
			"guidelines": "https://spectrum.adobe.com/page/radio-group",
			"rootClass": "spectrum-Radio",
			"swc": "https://opensource.adobe.com/spectrum-web-components/components/radio/"
		}
	],
	"peerDependenciesMeta": {
		"@spectrum-css/icon": {
			"optional": true
		},
		"@spectrum-css/tokens": {
			"optional": true
		}
	}
