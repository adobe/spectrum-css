		"./metadata.json": "./dist/metadata.json",
		"./package.json": "./package.json",
		"./stories/*": "./stories/*"
	},
	"main": "dist/index.css",
	"peerDependencies": {
		"@spectrum-css/icon": ">=9",
		"@spectrum-css/tokens": ">=14.0.0-next.0"
