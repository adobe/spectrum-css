{
	"name": "@spectrum-css/commons",
	"version": "12.0.0-next.0",
	"description": "Common mixins for Spectrum CSS components",
	"license": "Apache-2.0",
	"author": "Adobe",
	"homepage": "https://opensource.adobe.com/spectrum-css/",
	"repository": {
		"type": "git",
		"url": "https://github.com/adobe/spectrum-css.git",
		"directory": "components/commons"
	},
	"bugs": {
		"url": "https://github.com/adobe/spectrum-css/issues"
	},
	"exports": {
		".": "./index.css",
		"./*.md": "./*.md",
		"./package.json": "./package.json",
		"./*.css": "./*.css",
		"./stories/*": "./stories/*"
	},
	"main": "index.css",
	"files": [
		"*.md",
		"package.json"
	],
	"peerDependencies": {
		"@spectrum-css/tokens": ">=16.1.0-next.0"
	},
	"peerDependenciesMeta": {
		"@spectrum-css/tokens": {
			"optional": true
		}
	},
	"devDependencies": {
		"@spectrum-css/tokens": "16.1.0-next.9"
	},
	"keywords": [
		"design-system",
		"spectrum",
		"spectrum-css",
		"adobe",
		"adobe-spectrum",
		"css"
	],
	"publishConfig": {
		"access": "public"
	}
}
