		"@spectrum-css/opacitycheckerboard": "workspace:^",
		"@spectrum-css/tokens": "workspace:^"
	},
	"keywords": [
		"spectrum",
		"css",
		"design system",
		"adobe"
	],
	"publishConfig": {
